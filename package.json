{
    "name": "aurora-launcher",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "dev": "concurrently -n server,launcher \"npm:dev -w packages/server\" \"npm:dev -w packages/launcher\"",
        "build:libs": "npm run build -w packages/api -w packages/core"
    },
    "devDependencies": {
        "concurrently": "^8.0.1"
    }
}
