{
    "name": "@auroralauncher/server",
    "version": "0.1.0-dev",
    "branch": "dev",
    "description": "LauncherServer for AuroraLauncher",
    "keywords": [
        "minecraft",
        "launcher"
    ],
    "homepage": "https://aurora-launcher.ru",
    "bugs": {
        "url": "https://github.com/AuroraTeam/LauncherServer/issues"
    },
    "repository": "github:aurorateam/launcherserver",
    "license": "MIT",
    "author": "AuroraTeam",
    "contributors": [
        "JoCat (https://github.com/JoCat)",
        "LoomeL (https://github.com/LoomeL)",
        "FIXxp (https://github.com/FIXxp)",
        "IsTopNick (https://github.com/IsTopNick)"
    ],
    "main": "dist/app.js",
    "scripts": {
        "dev": "concurrently -i -n build,app -c blue.bold,yellow.bold \"nodemon -- --dev\" \"npm:build:dev -- --watch\"",
        "build:dev": "node build.mjs",
        "build:prod": "node build.mjs --sources-content=false --minify",
        "build:bin": "pkg dist/app.js -t node16-linux-x64,node16-macos-x64,node16-win-x64 --out-path dist --compress GZip",
        "clean": "rimraf dist",
        "prettier": "prettier --write src",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "start": "node ."
    },
    "devDependencies": {
        "@anatine/esbuild-decorators": "^0.2.19",
        "@types/adm-zip": "^0.5.0",
        "@types/cli-progress": "^3.11.0",
        "@types/hjson": "^2.4.3",
        "@types/lodash-es": "^4.17.6",
        "@types/node": "^18.8.3",
        "@types/prompts": "^2.4.1",
        "@types/semver": "^7.3.12",
        "@types/uuid": "^8.3.4",
        "@types/ws": "^8.5.3",
        "@typescript-eslint/eslint-plugin": "^5.39.0",
        "@typescript-eslint/parser": "^5.39.0",
        "concurrently": "^7.4.0",
        "esbuild": "^0.14.54",
        "eslint": "^8.25.0",
        "eslint-plugin-node": "^11.1.0",
        "import-sort-style-module": "^6.0.0",
        "minimist": "^1.2.7",
        "nodemon": "^2.0.20",
        "pkg": "^5.8.0",
        "prettier": "^2.7.1",
        "prettier-plugin-import-sort": "0.0.7",
        "rimraf": "^3.0.2",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "@auroralauncher/core": "^0.9.0",
        "adm-zip": "^0.5.9",
        "aurora-rpc-server": "^0.1.2",
        "chalk": "^5.1.0",
        "class-transformer": "^0.5.1",
        "cli-progress": "^3.11.2",
        "fast-querystring": "^1.0.0",
        "hjson": "^3.2.2",
        "lodash-es": "^4.17.21",
        "mysql2": "^2.3.3",
        "prompts": "^2.4.2",
        "raw-body": "^2.5.1",
        "reflect-metadata": "^0.1.13",
        "semver": "^7.3.8",
        "strip-ansi": "^7.0.1",
        "tsyringe": "^4.7.0",
        "typeorm": "^0.3.10",
        "uuid": "^9.0.0"
    },
    "importSort": {
        ".js, .mjs, .ts": {
            "style": "module",
            "parser": "typescript"
        }
    }
}
