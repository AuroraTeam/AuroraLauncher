{
    "name": "@aurora-launcher/core",
    "version": "0.23.0",
    "description": "Base library for Aurora Launcher",
    "type": "commonjs",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/types/index.d.ts",
    "exports": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "types": "./dist/types/index.d.ts"
    },
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "prettier": "prettier --write src",
        "lint": "eslint . --ext .ts",
        "lint:fix": "npm run lint -- --fix",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AuroraTeam/AuroraLauncher.git"
    },
    "author": "AuroraTeam",
    "contributors": [
        "JoCat (https://github.com/JoCat)"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/AuroraTeam/AuroraLauncher/issues"
    },
    "homepage": "https://github.com/AuroraTeam/AuroraLauncher#readme",
    "importSort": {
        ".js, .ts": {
            "style": "module",
            "parser": "typescript"
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "devDependencies": {
        "@tsconfig/node20": "^20.1.2",
        "@tsconfig/strictest": "^2.0.3",
        "@types/node": "^20.11.24",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "^7.1.1",
        "eslint": "^8.56.0",
        "import-sort-style-module": "^6.0.0",
        "prettier": "^3.2.5",
        "prettier-plugin-import-sort": "0.0.7",
        "tsup": "^8.0.2",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "adm-zip": "^0.5.10",
        "p-map": "^7.0.1",
        "undici": "^6.7.0"
    }
}
